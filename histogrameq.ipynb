    "import numpy as np\n",
    "import cv2 as cv\n",
    "from matplotlib import pyplot as plt\n",
    "img = cv.imread('yel.jpeg',0)\n",
    "equ = cv.equalizeHist(img)\n",
    "res = np.hstack((img,equ)) #stacking images side-by-side cv.imwrite('res.png',res)\n",
    "cv.imwrite('res.png',res)\n",
    "plt.show()"
   ]
